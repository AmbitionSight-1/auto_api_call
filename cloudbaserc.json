{
	"envId": "{{env.envId}}",
	"version": "2.0",
	"framework": {
		"name": "gh-actions-tencent",
		"plugins": {
			"func": {
				"use": "@cloudbase/framework-plugin-function",
				"inputs": {
					"functions": [{
						"name": "auto_ms_api",
						"memorySize": 64,
						"timeout": 5,
						"runtime": "CustomRuntime",
						"handler": "index.main",
						"envVariables": {
							"TC_SECRET_ID": "${{env.TC_SECRET_ID}}",
							"TC_SECRET_KEY": "${{env.TC_SECRET_KEY}}",
							"CLIENT_ID1":"${{env.CLIENT_ID1}}",
							"CLIENT_ID2":"${{env.CLIENT_ID2}}",
							"CLIENT_SECRET1":"${{env.CLIENT_SECRET1}}",
							"CLIENT_SECRET2":"${{env.CLIENT_SECRET2}}"
						}
					}]
				}
			}
		}
	}
}